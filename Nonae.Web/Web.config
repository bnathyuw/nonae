<?xml version="1.0"?>
<configuration>
  <system.web>
	<compilation debug="true" targetFramework="4.5" />
	<httpRuntime targetFramework="4.5" enableVersionHeader="false" />
  </system.web>
  <system.webServer>
	<handlers>
	  <clear/>
	  <add name="NonaeHttpHandler"
		   path="*"
		   verb="*"
		   type="Nonae.Web.HttpHandler"/>
	</handlers>
  </system.webServer>
</configuration>
